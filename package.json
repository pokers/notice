{
  "name": "notice",
  "version": "1.0.0",
  "description": "Example of notice",
  "main": "app.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pokers/notice.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pokers/notice/issues"
  },
  "scripts": {
    "test": "./node_modules/.bin/jest --forceExit --detectOpenHandles",
    "dev": "./node_modules/.bin/ts-node-dev --report-uncaught-exception --report-on-signal --report-on-fatalerror -- ./src/app.ts",
    "inspect": "./node_modules/.bin/ts-node-de --report-uncaught-exception --report-on-signal --report-on-fatalerror --inspect --watch -- ./src/app.ts",
    "compile": "rm -rf ./dist/* && ./node_modules/.bin/tsc",
    "bundle": "rm -rf ./dist/* && ./node_modules/.bin/esbuild ./src/app.ts --platform=node --bundle --outfile=./dist/app.js",
    "build": "rm -rf ./dist/* && ./node_modules/.bin/tsc && e./node_modules/.bin/sbuild ./src/app.ts --platform=node --bundle --outfile=./dist/app.js",
    "start": "node --es-module-specifier-resolution=node ./dist/app.js",
    "devjs": "./node_modules/.bin/node --report-uncaught-exception --report-on-signal --report-on-fatalerror -- ./dist/app.js",
    "inspectjs": "./node_modules/.bin/node --report-uncaught-exception --report-on-signal --report-on-fatalerror --inspect ./dist/app.js"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "heapdump": "^0.3.15",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "promise-mysql": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.20.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/heapdump": "^0.3.1",
    "@types/http-errors": "^1.8.2",
    "@types/jest": "^27.5.1",
    "@types/lodash": "^4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.33",
    "@types/supertest": "^2.0.12",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "babel-jest": "^28.1.0",
    "concurrently": "^7.2.0",
    "esbuild": "^0.14.41",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "prettier": "^2.6.2",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.2",
    "typescript": "^4.6.4"
  }
}
